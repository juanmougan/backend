<h1>Listado de Alumnos</h1>
<table>
  <thead>
    <tr>
      <!-- <th>CSV ID</th> -->
      <th>Nombre</th>
      <th>Apellido</th>
      <th>NÃºmero de Registro</th>
      <th>Carrera</th>
      <th>GCM regid</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <!-- <td><%= student.csv_id %></td> -->
        <td><%= student.first_name %></td>
        <td><%= student.last_name %></td>
        <td><%= student.file_number %></td>
        <td><%= student.career.code %> - <%= student.career.name %></td>
        <td><%= student.regid %></td>
        <td><%= student.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
