<h1>Listing Students</h1>
<table>
  <thead>
    <tr>
      <th>CSV ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>File Number</th>
      <th>Career</th>
      <th>GCM regid</th>
      <th>Email</th>
      <th>Enrollments</th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.csv_id %></td>
        <td><%= student.first_name %></td>
        <td><%= student.last_name %></td>
        <td><%= student.file_number %></td>
        <td><%= student.career.code %> - <%= student.career.name %></td>
        <td><%= student.regid %></td>
        <td><%= student.email %></td>
        <td><%= link_to 'Enrollments', student.enrollments %></td>
      </tr>
    <% end %>
  </tbody>
</table>
