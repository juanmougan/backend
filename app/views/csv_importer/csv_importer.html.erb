<%= form_tag({action: :import_csv}, multipart: true) do %>
  <label for="file">File to Upload</label> <%= file_field_tag "file" %>
  <%= label_tag(:file_name, "File name:") %>
  <%= text_field_tag(:file_name) %>
  <%= submit_tag("Upload") %>
<% end %>
